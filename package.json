{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "shazam",
  "title": "Shazam",
  "description": "Unofficial Shazam Extension",
  "icon": "command-icon.png",
  "author": "antonengelhardt",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Listen",
      "subtitle": "Start to listen and get result",
      "description": "Start to listen and receive the current title playing",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.67.1",
    "node-audiorecorder": "^1.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
